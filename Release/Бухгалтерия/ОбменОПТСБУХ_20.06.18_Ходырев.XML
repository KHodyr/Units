			СтрокаПервого  = ТаблицаДокументов.Получить(0);
			//Если СтрокаПервого.Ссылка.Дата &lt; Параметры.ДатаЗапретаВыгрузки Тогда Продолжить КонецЕсли;
			СтрокаПоследнего = ТаблицаДокументов.Получить(ТаблицаДокументов.Количество()-1);
			ДокументОбъект = СтрокаПоследнего.Ссылка.ПолучитьОбъект();
			ДокументОбъект.Товары.Загрузить(ТаблицаТоваров);
			ДокументОбъект.СуммаДокумента = ТаблицаТоваров.Итог("Сумма");
			ДокументОбъект.Пометкаудаления = Ложь;
		    ДокументОбъект.Записать(РежимЗаписиДокумента.Проведение,РежимПроведенияДокумента.Неоперативный);
			МассивОбработанных.Добавить(ДокументОбъект.НомерВходящегоДокумента);
			ТаблицаДокументов.Удалить(СтрокаПоследнего);
			
			Для Каждого СтрТаблицаДокументов ИЗ ТаблицаДокументов Цикл
				ДокументОбъект = СтрТаблицаДокументов.Ссылка.ПолучитьОбъект();
				ДокументОбъект.Записать(РежимЗаписиДокумента.ОтменаПроведения);
				ДокументОбъект.Пометкаудаления = Истина;
				ДокументОбъект.Записать(РежимЗаписиДокумента.Запись);
				СчетФактура = Документы.СчетФактураПолученный.НайтиПоРеквизиту("ДокументОснование",СтрТаблицаДокументов.Ссылка);
				Если ЗначениеЗаполнено(СчетФактура) Тогда 
					СчетФактураОбъект = СчетФактура.ПолучитьОбъект();
					СчетФактураОбъект.Записать(РежимЗаписиДокумента.ОтменаПроведения);
					СчетФактураОбъект.Пометкаудаления = Истина;
					СчетФактураОбъект.Записать(РежимЗаписиДокумента.Запись);
				КонецЕсли;	 
				
				
			КонецЦикла;	
		КонецЕсли;
	Конецесли;	
Исключение
		Параметры.СброситьУзел = Ложь;
		ЗаписьЖурналаРегистрации("Ошибка обмена.Процедура после загрузки данных.Ошибка записи поступления",УровеньЖурналаРегистрации.Ошибка,,,ОписаниеОшибки());
	КонецПопытки;	
КонецЦикла;

Для Каждого СтрСФ Из Параметры.ДокументыФормированичСФ Цикл
	Попытка
		СчетФактура = Документы.СчетФактураПолученный.НайтиПоРеквизиту("ДокументОснование",СтрСФ.Ссылка);
		Если НЕ ЗначениеЗаполнено(Документы.СчетФактураПолученный.НайтиПоРеквизиту("ДокументОснование",СтрСФ.Ссылка)) Тогда
			НовыйСЧ = Документы.СчетФактураПолученный.СоздатьДокумент();
			НовыйСЧ.Заполнить(СтрСФ.Ссылка);
			НовыйСЧ.ВидСчетаФактуры = Перечисления.ВидСчетаФактурыПолученного.НаПоступление;
			НовыйСЧ.КодВидаОперации = "01";
			НовыйСЧ.КодСпособаПолучения = 1;
			НовыйСЧ.НомерВходящегоДокумента  = СтрСФ.НомерСФ;
			НовыйСЧ.ДатаВходящегоДокумента  = СтрСФ.ДатаСФ;
			НовыйСЧ.Записать(РежимЗаписиДокумента.Проведение,РежимПроведенияДокумента.Неоперативный);
		Иначе 
			СчетФактураОбъект = СчетФактура.ПолучитьОбъект();
			СчетФактураОбъект.НомерВходящегоДокумента  = СтрСФ.НомерСФ;
			СчетФактураОбъект.ДатаВходящегоДокумента  = СтрСФ.ДатаСФ;
			СчетФактураОбъект.Записать(РежимЗаписиДокумента.Проведение,РежимПроведенияДокумента.Неоперативный);
		КонецЕсли;
	Исключение 
		Параметры.СброситьУзел = Ложь;
		ЗаписьЖурналаРегистрации("Ошибка обмена.Процедура после загрузки данных.Ошибка записи сч",УровеньЖурналаРегистрации.Ошибка,,,ОписаниеОшибки());
	КонецПопытки; 
	
КонецЦикла;</Текст>
